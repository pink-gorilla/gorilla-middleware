{:webly {:title "Webly NRepl Demo"
         ; clj
         :ns-clj [demo.service]
         ; cljs
         :ns-cljs [demo.page.main
                   demo.page.notebook
                   demo.page.ops
                   demo.page.info
                   demo.conn
                   goldly-server.app]
         :routes {:app {"" :demo/main
                        "ops" :demo/ops
                        "notebook" :demo/notebook
                        "info" :demo/info}
                  :api {}}
         :start-user-app [:goldly-server/init]
         ;:start-service pinkgorilla.nrepl.service/start-nrepl
         }

 :goldly {:enabled true
          :lazy true
          :routes {"" :demo/main
                   "ops" :demo/ops
                   "notebook" :demo/notebook
                   "info" :demo/info}}

 :google-analytics {:enabled true} ; set to false to disable google-analytics tracking.

 :timbre-loglevel [[#{"webly.ws.*"} :debug]
                   [#{"webly.user.*"} :info]
                   [#{"webly.*"} :info]
                   [#{"goldly.*"} :info]
                   [#{"org.eclipse.jetty.*"} :info]
                   [#{"picasso.*"} :info]
                   [#{"ui.notebook.*"} :info]
                   [#{"notebook.*"} :info]
                   [#{"*"} :debug]]

 :settings {:use-localstorage  true  ; allow user to change settings via localstorage
            :bongo 777}

 ;
 }